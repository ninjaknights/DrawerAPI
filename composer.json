{
  "name": "ninjaknights/drawerapi",
  "description": "PocketMine-MP virion designed to draw shapes and text in the world using the ServerScriptDebugDrawerPacket.",
  "type": "library",
  "version": "0.0.1",
  "license": "MIT",
  "require-dev": {
    "pocketmine/pocketmine-mp": "^5.0.0"
  },
  "autoload": {
    "classmap": ["src"],
    "psr-4": {
      "DrawerAPI\\": "src/"
    }
  },
  "scripts": {
    "compile": "php build-phar.php"
  },
  "extra": {
    "virion": {
      "spec": "3.1",
      "namespace-root": "NinjaKnights\\DrawerAPI"
    }
  },
  "authors": [
    {
      "name": "HydroGamesDev",
      "homepage": "https://github.com/HydroGamesDev",
      "role": "Developer",
      "email": "hydrogamesnetwork@gmail.com"
    }
  ]
}